<h1>Bloggenres#index</h1>
<p>※ジャンル・データの一覧</p>
<table>
  <tr>
    <th>Id</th><th>name</th><th>memo</th><th></th>
  </tr>
  <% @bloggenres.each do |bloggenre| %>
    <tr>
      <td><%= bloggenre.id %></td>
      <td><%= bloggenre.name %> %></td>
      <td><%= bloggenre.memo %></td>
      <td><%= link_to('Edit', "/bloggenres/#{bloggenre.id}/edit") %></td>
    </tr>
  <% end %>
</table>
<p class="link"><%= link_to('新規作成＞＞', '/bloggenres/new') %></p>
