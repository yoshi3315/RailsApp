<h1>People#index</h1>
<p><%= @msg %></p>
<pre>
  <table>
    <tr>
      <th>Id</th><th>Name</th>
    </tr>
    <% @data.each do |obj| %>
      <tr>
        <td><%= obj.id %></td>
        <td><a href='/people/<%= obj.id %>'><%= obj.name %></a></td>
        <td><a href='/people/edit/<%= obj.id %>'>Edit</a></td>
      </tr>
    <% end %>
  </table>
</pre>
