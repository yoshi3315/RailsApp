<p id="notice"><%= notice %></p>

<h1>Questionary Results</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questionaries.each do |questionary| %>
      <tr>
        <td><%= questionary.id %></td>
        <td><%= questionary.title %></td>
        <td><%= link_to 'Show', questionary_result %></td>
      </tr>
    <% end %>
  </tbody>
</table>
