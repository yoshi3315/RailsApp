<p id="notice"><%= notice %></p>

<h1>Questionary Results</h1>

<table>
  <thead>
    <tr>
      <th>Questionary</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questionary_results.each do |questionary_result| %>
      <tr>
        <td><%= questionary_result.questionary_id %></td>
        <td><%= questionary_result.result %></td>
        <td><%= link_to 'Show', questionary_result %></td>
        <td><%= link_to 'Edit', edit_questionary_result_path(questionary_result) %></td>
        <td><%= link_to 'Destroy', questionary_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Questionary Result', new_questionary_result_path %>
